{
  "name": "@vymalo/medusa-printful",
  "version": "1.0.10-rc.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vymalo/medusajs.git",
    "directory": "packages/vymalo-printful"
  },
  "main": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./api/*": "./dist/api/*.js",
    "./subscribers/*": "./dist/subscribers/*.js",
    "./workflows/*": "./dist/workflows/*.js",
    "./jobs/*": "./dist/jobs/*.js",
    "./*": "./dist/modules/*/index.js"
  },
  "scripts": {
    "prod:build": "rm -rf {dist,tsconfig.tsbuildinfo} && tsc --build",
    "gen:printful": "cross-env rimraf src/core/generated/printful && openapi-ts",
    "build": "npm run gen:printful && npm run prod:build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "medusa",
    "medusajs",
    "medusa-printful",
    "printful"
  ],
  "author": {
    "name": "Vymalo",
    "email": "selastlambou@gmail.com",
    "url": "https://github.com/vymalo"
  },
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@medusajs/core-flows": "^2.6.1",
    "@medusajs/framework": "^2.7.0",
    "@medusajs/types": "^2.8.0",
    "@medusajs/utils": "^2.6.1",
    "axios": "^1.8.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@hey-api/client-axios": "^0.6.2",
    "@hey-api/openapi-ts": "^0.64.13",
    "@medusajs/admin-sdk": "^2.6.1",
    "@medusajs/js-sdk": "^2.6.1",
    "@medusajs/ui": "^4.0.7",
    "@tanstack/react-query": "^5.71.5",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19",
    "cross-env": "^7.0.3",
    "react": "^19.1.0",
    "react-router-dom": "^7.4.1",
    "rimraf": "^6.0.1"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@medusajs/admin-sdk": "*",
    "@medusajs/ui": "*",
    "react": "*"
  }
}
