{
	"name": "@vymalo/medusa-webauthn",
	"version": "1.0.1",
	"description": "A starter for Medusa projects.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vymalo/medusajs.git",
		"directory": "packages/vymalo-webauthn"
	},
